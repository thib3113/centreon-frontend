{
  "jsc": {
    "externalHelpers": true,
    "loose": true,
    "minify": {
      "compress": {
        "unused": true
      }
    },
    "parser": {
      "dynamicImport": true,
      "syntax": "typescript",
      "tsx": true
    },
    "target": "es5",
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    }
  }
}